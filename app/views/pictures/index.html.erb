<h1>Pictures#index</h1>
<p>Find me in app/views/pictures/index.html.erb</p>

<ul>
    <% @pictures.each do |picture_inst|%>
        <div><img src=<%=picture_inst.image_url%>  width="500" height="600"/></div>
        <div><%= link_to picture_inst.title, picture_path(picture_inst) %></div>
        <%end%>
        
</ul>